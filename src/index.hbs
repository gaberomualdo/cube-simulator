    <!-- Viewport Meta Tags -->
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <!-- Title -->
    <title>Rubik's Cube</title>

    <!-- Favicons -->
    <link rel="apple-touch-icon" sizes="57x57" href="apple-icon-57x57.png" />
    <link rel="apple-touch-icon" sizes="60x60" href="apple-icon-60x60.png" />
    <link rel="apple-touch-icon" sizes="72x72" href="apple-icon-72x72.png" />
    <link rel="apple-touch-icon" sizes="76x76" href="apple-icon-76x76.png" />
    <link rel="apple-touch-icon" sizes="114x114" href="apple-icon-114x114.png" />
    <link rel="apple-touch-icon" sizes="120x120" href="apple-icon-120x120.png" />
    <link rel="apple-touch-icon" sizes="144x144" href="apple-icon-144x144.png" />
    <link rel="apple-touch-icon" sizes="152x152" href="apple-icon-152x152.png" />
    <link rel="apple-touch-icon" sizes="180x180" href="apple-icon-180x180.png" />

    <link rel="icon" type="image/png" sizes="192x192" href="android-icon-192x192.png" />
    <link rel="icon" type="image/png" sizes="32x32" href="favicon-32x32.png" />
    <link rel="icon" type="image/png" sizes="96x96" href="favicon-96x96.png" />
    <link rel="icon" type="image/png" sizes="16x16" href="favicon-16x16.png" />

    <meta name="msapplication-TileImage" content="ms-icon-144x144.png" />

    <!-- Theme color -->
    <meta name="msapplication-TileColor" content="#5a747b" />
    <meta name="theme-color" content="#5a747b" />

    <!-- PWA -->
    <link rel="manifest" href="manifest.json" />

    <!-- Main stylesheet -->
    <link rel="stylesheet" href="main.css?v={{cache_version}}" />

    <!-- Icon font -->
    <link rel="stylesheet" href="https://cdn.iconmonstr.com/1.3.0/css/iconmonstr-iconic-font.min.css" />
  </head>
  <body>
    <div class="container">
      <div class="col">
        <div class="logo">
          <img src="icon.png" alt="Rubik's Cube Logo" />
          <h1>Rubik's Cube</h1>
        </div>

        <div class="set-pieces">
          <button class="label">
            <p>Set Pieces</p>
            <i class="icon im im-angle-down"></i>
          </button>
        </div>

        <div class="moves">
          <ul class="row">
            <li class="move">
              <button>U</button>
              <p>or press <strong>q</strong></p>
            </li>
            <li class="move">
              <button>L</button>
              <p>or press <strong>w</strong></p>
            </li>
            <li class="move">
              <button>F</button>
              <p>or press <strong>e</strong></p>
            </li>
          </ul>
          <ul class="row">
            <li class="move">
              <button>U'</button>
              <p>or press <strong>a</strong></p>
            </li>
            <li class="move">
              <button>L'</button>
              <p>or press <strong>s</strong></p>
            </li>
            <li class="move">
              <button>F'</button>
              <p>or press <strong>d</strong></p>
            </li>
          </ul>
          <ul class="row">
            <li class="move">
              <button>R</button>
              <p>or press <strong>u</strong></p>
            </li>
            <li class="move">
              <button>B</button>
              <p>or press <strong>i</strong></p>
            </li>
            <li class="move">
              <button>D</button>
              <p>or press <strong>o</strong></p>
            </li>
          </ul>
          <ul class="row">
            <li class="move">
              <button>R'</button>
              <p>or press <strong>j</strong></p>
            </li>
            <li class="move">
              <button>B'</button>
              <p>or press <strong>k</strong></p>
            </li>
            <li class="move">
              <button>D'</button>
              <p>or press <strong>l</strong></p>
            </li>
          </ul>
        </div>
      </div>

      <div class="col">
        <div class="row">
          <img src="placeholder-image.png" alt="Rubik's Cube Front View" class="cube-image front-view" />
          <p class="caption">Front View</p>
        </div>
        <div class="row">
          <img src="placeholder-image.png" alt="Rubik's Cube Back View" class="cube-image back-view" />
          <p class="caption">Back View</p>
        </div>
      </div>
    </div>

    <!-- Main script -->
    <script src="main.js?v={{cache_version}}"></script>
  </body>
</html>
