<head>
    <!-- Viewport Meta Tags -->
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <!-- Title -->
    <title>Rubik's Cube</title>

    <!-- SEO -->
    <script type="application/ld+json">
    {
      "@type": "WebSite",
      "@context": "http://schema.org",
      "url": "{{siteDetails.url}}",
      "name": "{{siteDetails.name}}",
      "author": {
        "@type": "Person",
        "name": "{{siteDetails.author.name}}",
        "email": "{{siteDetails.author.email}}"
      },
      "description": "{{siteDetails.description}}"
    }
    </script>

    <meta http-equiv="X-UA-Compatible" content="ie=edge">

    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="white">

    <meta name="format-detection" content="telephone=no">

    <meta name="robots" content="index, follow">

    <meta name="author" content="{{siteDetails.author.name}}">
    <meta name="description" content="{{siteDetails.description}}">

    <meta name="twitter:card" content="summary_large_image">
    <meta name="twitter:title" content="{{siteDetails.name}}">
    <meta name="twitter:description" content="{{siteDetails.description}}">

    <meta property="og:title" content="{{siteDetails.name}}">
    <meta property="og:site_name" content="{{siteDetails.name}}">
    <meta property="og:description" content="{{siteDetails.description}}">

    <!-- Favicons -->
    <link rel="apple-touch-icon" sizes="57x57" href="apple-icon-57x57.png" />
    <link rel="apple-touch-icon" sizes="60x60" href="apple-icon-60x60.png" />
    <link rel="apple-touch-icon" sizes="72x72" href="apple-icon-72x72.png" />
    <link rel="apple-touch-icon" sizes="76x76" href="apple-icon-76x76.png" />
    <link rel="apple-touch-icon" sizes="114x114" href="apple-icon-114x114.png" />
    <link rel="apple-touch-icon" sizes="120x120" href="apple-icon-120x120.png" />
    <link rel="apple-touch-icon" sizes="144x144" href="apple-icon-144x144.png" />
    <link rel="apple-touch-icon" sizes="152x152" href="apple-icon-152x152.png" />
    <link rel="apple-touch-icon" sizes="180x180" href="apple-icon-180x180.png" />

    <link rel="icon" type="image/png" sizes="192x192" href="android-icon-192x192.png" />
    <link rel="icon" type="image/png" sizes="32x32" href="favicon-32x32.png" />
    <link rel="icon" type="image/png" sizes="96x96" href="favicon-96x96.png" />
    <link rel="icon" type="image/png" sizes="16x16" href="favicon-16x16.png" />

    <meta name="msapplication-TileImage" content="ms-icon-144x144.png" />

    <!-- Theme color -->
    <meta name="msapplication-TileColor" content="#5a747b" />
    <meta name="theme-color" content="#5a747b" />

    <!-- PWA -->
    <link rel="manifest" href="manifest.json" />

    <!-- Main stylesheet -->
    <link rel="stylesheet" href="main.css?v={{cacheVersion}}" />

    <!-- Icon font -->
    <link rel="stylesheet" href="https://cdn.iconmonstr.com/1.3.0/css/iconmonstr-iconic-font.min.css" />
  </head>
  <body>
    <div class="container">
      <div class="col">
        <div class="logo">
          <img src="icon.png" alt="Rubik's Cube Logo" />
          <h1>Rubik's Cube</h1>
        </div>

        <div class="set-pieces">
          <button class="label">
            <p>Set Pieces</p>
            <i class="icon im im-angle-down"></i>
          </button>
          <div class="content">
            {{#times 6}}
            <div class="face">
                {{#times 3}}
                <div class="row">
                  {{#times 3}}
                  <div class="piece"><i class="icon im im-sync"></i></div>
                  {{/times}}
                </div>
                {{/times}}
            </div>
            {{/times}}
          </div>
        </div>

        <div class="moves">
          <button class="label">
            <p>Moves</p>
            <i class="icon im im-angle-down"></i>
          </button>
          <div class="content">
            <ul class="misc-buttons">
              <li>
                <button>Solve Cube</button>
                <p>Solve the cube automatically with a basic solving algorithm.</p>
              </li>
              <li>
                <button>Scramble Cube</button>
                <p>Scramble the cube with 25 random moves.</p>
              </li>
              <li>
                <button>Undo</button>
                <p>Undo the previous move or action (ex: scramble or solve).</p>
              </li>
            </ul>
            <div class="move-buttons">
              {{#generateMovesMarkup cubeMoves}}
                <li class="move">
                  <button>{{@move.notation}}</button>
                  <p>or press <strong>{{#replace 'shift' '<span class="shift">shift</span>'}}{{@move.key}}{{/replace}}</strong></p>
                </li>
              {{/generateMovesMarkup}}
            </div>
            <ul class="log"></ul>
          </div>
        </div>
      </div>

      <div class="col">
        <div class="row">
          <img src="placeholder-image.png" alt="Rubik's Cube Front View" class="cube-image front-view" />
          <p class="caption">Front View</p>
        </div>
        <div class="row">
          <img src="placeholder-image.png" alt="Rubik's Cube Back View" class="cube-image back-view" />
          <p class="caption">Back View</p>
        </div>
      </div>
    </div>

    <!-- Main script -->
    <script src="main.js?v={{cacheVersion}}"></script>

    <!-- Moment.js -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.25.3/moment.min.js"></script>
  </body>
</html>
